{
    "name": "catch/ssmasterlib-forms",
    "description": "Allows you to create forms in SilverStripe that are compatible with the 2DM MasterLib.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "master library", "forms"],
	"license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Aaron Latham-Anderson",
            "email": "aaron@catch.co.nz"
        }
    ],
    "require":
    {
        "silverstripe/framework": "^4.3",
        "silverstripe/cms": "^4.3",
        "silverstripe/vendor-plugin": "^1",
        "studiobonito/silverstripe-spamprotection-honeypot" : "~2"
    },
    "replace": {
        "2degrees/ssmasterlib-forms": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "CatchDesign\\SSMasterLibForms\\": "code/"
        }
    }
}
